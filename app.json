{
  "expo": {
    "name": "quiz_app",
    "slug": "quiz_app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "src/assets/icons/QuizGenAI.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 10,
      "adaptiveIcon": {
        "foregroundImage": "src/assets/icons/QuizGenAI.png",
        "backgroundColor": "#4E2245"
      },
      "package": "com.luiz_coppini.quiz_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-splash-screen",
        {
          "image": "src/assets/icons/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#271C3A"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "7f6913b6-47d0-4f03-9a22-1519bbbd7da4"
      }
    },
    "assetBundlePatterns": [
      "assets/**/*",
      "src/assets/**/*"
    ]
  }
}